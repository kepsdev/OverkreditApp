{
  "expo": {
    "name": "Overkredit",
    "slug": "Overkredit",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/splash/Geeks.png",
    "splash": {
      "image": "./src/assets/splash/splash.png",
      "resizeMode": "cover"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
