    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/splash/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./src/assets/splash/favicon.png"
    }
  }
}
